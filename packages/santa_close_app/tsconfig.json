{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["es2017"],
    "jsx": "react-native",
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
